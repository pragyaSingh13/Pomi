<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.bucket.BucketFragment">

    <ListView
        android:id="@+id/bucketList"
        android:layout_width="350dp"
        android:layout_height="492dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="77dp"
        android:background="@drawable/listbackground"
        app:layout_constraintBottom_toTopOf="@+id/gifImageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.189">

    </ListView>

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView5"
        android:layout_width="220dp"
        android:layout_height="211dp"
        android:layout_marginTop="56dp"
        android:src="@drawable/bucket"
        app:layout_constraintEnd_toEndOf="@+id/bucketList"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName2"
        app:layout_constraintTop_toBottomOf="@+id/bucketList" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="248dp"
        android:layout_height="46dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="164dp"
        android:background="@drawable/listbackground"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="@+id/bucketList"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bucketList"
        app:layout_constraintTop_toBottomOf="@+id/bucketList" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button3"
        android:layout_width="67dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/smoothbutton"
        android:text="SAVE"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/gifImageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.815"
        app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName2"
        app:layout_constraintTop_toBottomOf="@+id/bucketList"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>